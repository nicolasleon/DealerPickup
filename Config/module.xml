<?xml version="1.0" encoding="UTF-8"?>
<module xmlns="http://thelia.net/schema/dic/module"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://thelia.net/schema/dic/module http://thelia.net/schema/dic/module/module-2_2.xsd">
    <fullnamespace>DealerPickup\DealerPickup</fullnamespace>
    <descriptive locale="en_US">
        <title>Stores pickup</title>
        <description>Allow a customer to pickup her order from a list of stores</description>
    </descriptive>
    <descriptive locale="fr_FR">
        <title>Retrait en magasin</title>
        <description>Offre la possibilité de retirer une commande dans le point de vente de son choix</description>
    </descriptive>

    <languages>
        <language>en_US</language>
        <language>fr_FR</language>
    </languages>
    <version>1.0.0</version>
    <authors>
        <author>
            <name>Nicolas Léon</name>
            <email>nicolas@omnitic.com</email>
        </author>
    </authors>
    <type>delivery</type>

    <!-- module dependencies -->
    <required>
        <module version="~2.5">Dealer</module>
    </required>

    <thelia>2.2.0</thelia>
    <stability>other</stability>
    <mandatory>0</mandatory>
    <hidden>0</hidden>
</module>
